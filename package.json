{
  "name": "grammar-well",
  "version": "1.0.0",
  "description": "Simple, fast, powerful parser toolkit for JavaScript.",
  "main": "build/index.js",
  "scripts": {
    "build": "tsc",
    "cli": "ts-node ./src/cli",
    "bootstrap": "mocha legacy-test/bootstrap.test.js && bin/nearleyc.js src/grammars/nearley.ne > tmp && mv tmp lib/nearley-language-bootstrapped.js && echo bootstrapped ok",
    "benchmark": "benchr legacy-test/benchmark.js",
    "test": "mocha legacy-test/*.test.js",
    "profile": "build/cli.js legacy-test/grammars/parens.ne > legacy-test/grammars/parens.js && node legacy-test/profile.js"
  },
  "author": "0x6563",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/0x6563/grammar-well.git"
  },
  "dependencies": {
    "moo": "^0.5.1"
  },
  "devDependencies": {
    "@types/moo": "^0.5.5",
    "@types/node": "^17.0.25",
    "babel-cli": "^6.18.0",
    "babel-preset-env": "^1.6.0",
    "benchr": "^4.3.0",
    "coffee-script": "^1.12.7",
    "expect": "^27.5.1",
    "mocha": "^9.2.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "keywords": [
    "parser",
    "parse",
    "generator",
    "compiler",
    "compile",
    "grammar",
    "language"
  ]
}
